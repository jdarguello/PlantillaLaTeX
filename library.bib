keywords = {Agro-industrial biomasses,Crude glycerol,Energy density,Impact resistance,Pelletizing},
pages = {558--568},
publisher = {Elsevier Ltd},
title = {{Pelletizing of Colombian agro-industrial biomasses with crude glycerol}},
